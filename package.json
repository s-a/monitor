{
  "name": "pm2-networks-monitor",
  "version": "3.1.8",
  "description": "pm2-networks-monitor",
  "config": {
    "days_interval": 7,
    "max_size": 5242880
  },
  "scripts": {
    "build": "cd frontend/monitor && node node_modules/react-scripts/bin/react-scripts.js build"
  },
  "apps": [
    {
      "script": "index.js",
      "merge_logs": true,
      "max_memory_restart": "200M"
    }
  ],
  "dependencies": {
    "@pm2/io": "^2.4.5",
    "@slack/client": "^4.8.0",
    "bunyan": "^1.8.12",
    "bunyan-format": "^0.2.1",
    "cheerio": "^1.0.0-rc.2",
    "diskusage": "^0.2.5",
    "elasticsearch": "^15.1.1",
    "find-root": "^1.1.0",
    "handlebars": "^4.0.12",
    "http-proxy": "^1.17.0",
    "jsonwebtoken": "^8.3.0",
    "moment": "^2.22.2",
    "mongodb": "^3.1.8",
    "nodemailer": "^4.6.8",
    "object-hash": "^1.3.0",
    "pm2": "^3.2.2",
    "pm2-events": "^0.1.1",
    "read-last-lines": "^1.6.0",
    "restify": "^7.2.1",
    "systeminformation": "^3.45.9",
    "websocket": "^1.0.28"
  },
  "devDependencies": {
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-react": "^7.11.1",
    "eslint-plugin-standard": "^4.0.0"
  }
}
